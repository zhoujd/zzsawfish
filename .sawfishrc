;;;This is sawfish configure for zhoujd
;;;Create by zhoujd on 20120803

;;zzsawfish home
(defvar zzsawfish-path "~/zzsawfish/")

;;add load-path
(setq load-path 
     (append (list
              (concat zzsawfish-path "lisp")
              (concat zzsawfish-path "configure"))
              load-path))

;;add theme-load-path
(setq theme-load-path 
      (append (list
               (concat zzsawfish-path "theme"))
              theme-load-path))

;;load self setting
(mapcar require
        '(
          common-setting
          other-setting
          key-setting
          ))

;;;~/.sawfishrc end here
