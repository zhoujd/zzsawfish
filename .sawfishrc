;;zzsawfish home
(defvar zzsawfish-path "~/zzsawfish/")

;;add load-path
(setq load-path 
     (append (list
              (concat zzsawfish-path "lisp")
              (concat zzsawfish-path "configure"))
              load-path))

;;add theme-load-path
(setq theme-load-path 
      (append (list
               (concat zzsawfish-path "theme"))
              theme-load-path))

(require 'common-setting)
(require 'other-setting)
(require 'key-setting)

